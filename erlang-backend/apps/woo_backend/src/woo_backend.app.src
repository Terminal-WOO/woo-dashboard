%% -*- erlang -*-
{application, woo_backend,
 [{description, "WOO Dashboard Erlang Backend - OTP Application"},
  {vsn, "1.0.0"},
  {registered, [woo_sup, woo_document_manager, woo_event_manager]},
  {mod, {woo_backend_app, []}},
  {applications,
   [kernel,
    stdlib,
    sasl,
    cowboy,
    jsx
   ]},
  {env, [
      {http_port, 8080},
      {max_documents, 1000}
  ]},
  {modules, []},
  {licenses, ["MIT"]},
  {links, []}
 ]}.
